{
  "name": "underflag-mongodb",
  "version": "1.0.3",
  "description": "mongodb provider for underflag",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "test": "jest",
    "prepublish": "npm run test && npm run build",
    "example": "npx ts-node examples/mongodb",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dieg0s0uza/underflag-mongodb.git"
  },
  "keywords": [
    "feature flag",
    "feature toggle",
    "remote config",
    "mongodb"
  ],
  "author": "Diego Souza",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dieg0s0uza/underflag-mongodb/issues"
  },
  "homepage": "https://github.com/dieg0s0uza/underflag-mongodb#readme",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "dist"
    ]
  },
  "files": [
    "dist/**/*"
  ],
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.8",
    "husky": "^7.0.4",
    "jest": "^27.4.3",
    "mongodb-memory-server": "^8.1.0",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "typescript": "^4.5.2",
    "underflag": "^2.1.0"
  },
  "dependencies": {
    "mongodb": "^4.3.0"
  },
  "peerDependencies": {
    "underflag": "^2.1.0"
  }
}